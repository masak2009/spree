<h1><%= t("new_tax_rate") %></h1>

<%= error_messages_for :tax_rate %>
  <%= link_to t('ext.additional_translation.back_to_list'), { :action => :index } %>

<% form_for(:tax_rate, :url => collection_url) do |f| %>
