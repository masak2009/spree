<h1><%= t("editing_coupon") %></h1>
<%= error_messages_for :coupon %>
  <%= link_to t('ext.additional_translation.back_to_list'), { :action => :index } %>
<% form_for(@coupon, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update")%>
  </p>
<% end %>
